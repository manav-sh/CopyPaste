{
    "name": "CopyPaste",
    "description": "Simple demo extension",
    "version": "1.1",
    "manifest_version": 3,
    "permissions": [
        "clipboardRead"
    ],
    "host_permissions": [],
    "background": {
    "service_worker": "background.js"
    },
    "action": {
    "default_title": "Cryptobase",
    "default_icon": "Fire.png",
    "default_popup": "popup.html"
    },
    "icons": {  
    "16": "Fire.png",
    "32": "Fire.png",
    "48": "Fire.png",
    "128": "Fire.png"
    }
}